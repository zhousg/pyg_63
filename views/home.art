{{extend './layout/common.art'}}
{{block 'styles'}}
<link rel="stylesheet" href="/assets/css/page-home.css">
{{/block}}
{{block 'scripts'}}
<script src="/assets/js/sui.carousel.js"></script>
<script>
  $(function () {
    $('#likeBtn').on('click', function () {
      //获取猜你喜欢数据
      $.get('/like', function (res) {
        //拼接HTML格式字符串
        // let html = ''
        // res.data.forEach(function ($value, i) {
        //   html += `
        //     <li class="yui3-u-1-6">
        //         <a href="/item/${$value.id}" class="pic"><img src="${$value.thumbnail}"></a>
        //         <p>${$value.name}</p>
        //         <h3>¥${$value.price}</h3>
        //     </li>
        //     `
        // })

        // res.data.reduce()
        // reduce(每次遍历的回调函数,起始遍历的上一次返回值)
        // 回调函数(prev,$value) prev上一次变量的返回结构  $value 当前遍历的对象
        let html = res.data.reduce(function (prev,$value) {
          return prev + `
                <li class="yui3-u-1-6">
                    <a href="/item/${$value.id}" class="pic"><img src="${$value.thumbnail}"></a>
                    <p>${$value.name}</p>
                    <h3>¥${$value.price}</h3>
                </li>
                `
        },'')
        

        // res.data.map()
        // let html = res.data.map(function ($value,i) {
        //     return `
        //             <li class="yui3-u-1-6">
        //                 <a href="/item/${$value.id}" class="pic"><img src="${$value.thumbnail}"></a>
        //                 <p>${$value.name}</p>
        //                 <h3>¥${$value.price}</h3>
        //             </li>
        //             `
        // }).join('')


        //渲染
        $('#picLBxxl').fadeOut(function () {
          $(this).html(html).fadeIn()
        })
      })
    })
  })
</script>
{{/block}}
{{block 'content'}}
